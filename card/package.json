{
	"name": "dahua-doorbell",
	"version": "1.0.0",
	"description": "Custom card for Home Assistant to integrate with Dahua Doorbells through an Asterisks server.",
	"author": "Brian Burton",
	"source": "src/global/dahua-doorbell.ts",
	"module": "../custom_components/dahua_doorbell/js/dahua-doorbell.js",
	"targets": {
		"global": {
			"source": "src/global/dahua-doorbell.ts",
			"isLibrary": false,
			"context": "browser",
			"outputFormat": "esmodule",
			"distDir": "../custom_components/dahua_doorbell/dist"
		},
		"settings": {
			"source": "src/settings/dahua-doorbell-settings.ts",
			"isLibrary": false,
			"context": "browser",
			"outputFormat": "esmodule",
			"distDir": "../custom_components/dahua_doorbell/dist"
		}
	},
	"scripts": {
		"watch": "parcel watch",
		"build": "parcel build"
	},
	"devDependencies": {
		"@parcel/transformer-inline-string": "^2.8.3",
		"@parcel/transformer-sass": "^2.12.0",
		"events": "^3.3.0",
		"parcel": "^2.8.3",
		"process": "^0.11.10",
		"sass": "^1.75.0"
	},
	"dependencies": {
		"@mdi/js": "^7.4.47",
		"custom-card-helpers": "^1.9.0",
		"home-assistant-js-websocket": "^8.0.1",
		"jssip": "^3.10.1",
		"lit": "^2.7.4",
		"rxjs": "^7.8.1"
	}
}
